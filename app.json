{
  "expo": {
    "name": "Manifestation Alarm",
    "slug": "manifestation-alarm",
    "version": "1.0.6",
    "orientation": "portrait",
    "sdkVersion": "53.0.0",
    "icon": "./assets/images/icons/manifestation-alarm-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manifestationalarm.app",
      "infoPlist": {
        "UIBackgroundModes": ["audio", "background-fetch", "background-processing"],
        "NSUserNotificationUsageDescription": "This app uses notifications to remind you to track your mood and practice manifestations.",
        "UIUserInterfaceStyle": "Light",
        "UNUserNotificationCenter": true
      }
    },
    "android": {
      "package": "com.manifestationalarm.app",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.USE_FULL_SCREEN_INTENT"
      ]
    },
    "web": {
      "favicon": "./assets/images/icons/favicon.png"
    },
    "notifications": {
      "icon": "./assets/images/icons/manifestation-alarm-192.png",
      "color": "#6366f1",
      "sounds": [
        "./assets/sounds/ambient_piano.mp3",
        "./assets/sounds/singing_bowl.mp3",
        "./assets/sounds/singing_bowl_hit.mp3",
        "./assets/sounds/tibetan_bowl_low.mp3",
        "./assets/sounds/calm_music.mp3",
        "./assets/sounds/relaxing_guitar.mp3"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icons/manifestation-alarm-192.png",
          "color": "#6366f1",
          "sounds": [
            "./assets/sounds/ambient_piano.mp3",
            "./assets/sounds/singing_bowl.mp3",
            "./assets/sounds/singing_bowl_hit.mp3",
            "./assets/sounds/tibetan_bowl_low.mp3",
            "./assets/sounds/calm_music.mp3",
            "./assets/sounds/relaxing_guitar.mp3"
          ]
        }
      ]
    ],
    "extra": {
      "environment": "development"
    },
    "updates": {
      "enabled": false
    }
  }
}
